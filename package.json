{
    "name": "formed_church",
    "version": "1.0.0",
    "description": "This is a website being created for 'Formed Church', a new church being started in St. Pete, Florida",
    "main": "index.html",
    "scripts": {
        "build": "npm run clean && npm run imagemin && npm run copyfonts && npm run usemin",
        "clean": "rimraf dist",
        "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts && copyfiles -f node_modules/@fortawesome/fontawesome-free/css/* dist/fonts",
        "imagemin": "imagemin img/* -o dist/img",
        "lite": "lite-server",
        "start": "npm run watch:all",
        "test": "echo \"Error: no test specified\" && exit 1",
        "usemin": "usemin contactus.html -d dist --htmlmin -o dist/contactus.html && usemin staff.html -d dist --htmlmin -o dist/staff.html && usemin index.html -d dist --htmlmin -o dist/index.html && usemin whyformedchurch.html -d dist --htmlmin -o dist/whyformedchurch.html && usemin js/intersection-observer.js -d dist -o dist/js/intersection-observer.js",
        "scss": "node-sass -o css/ css/",
        "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
        "watch:all": "parallelshell \"npm run watch:scss\" \"npm run lite\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/drewjtaylor/formed_church.git"
    },
    "author": "Andrew Taylor",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/drewjtaylor/formed_church/issues"
    },
    "homepage": "https://github.com/drewjtaylor/formed_church#readme",
    "devDependencies": {
        "copyfiles": "^2.2.0",
        "imagemin-cli": "^5.1.0",
        "lite-server": "^2.6.1",
        "node-sass": "^6.0.1",
        "onchange": "^7.0.0",
        "parallelshell": "3.0.1",
        "rimraf": "^3.0.2",
        "usemin-cli": "^0.6.0"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.1.1",
        "bootstrap": "^4.5.2",
        "bootstrap-social": "^5.1.1",
        "font-awesome": "^4.7.0",
        "jquery": "^3.5.1",
        "popper.js": "^1.16.1",
        "uglify-js": "^3.15.5"
    }
}
